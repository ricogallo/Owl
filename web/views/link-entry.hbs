<div class="row">
  <div class="span1">
    <img class="numbee-thumb" src="https://si0.twimg.com/profile_images/2201732897/notch_weird_reasonably_small.png">
  </div>
  <div class="span7">
    <strong>@{{link.user_id}}</strong>
    <div style="margin-top: 5px">
      <a href="{{link.uri}}">{{link.uri}}</a>
      <div style="margin-top: 5px">
      {{#each link.tags}}
        <a href="#">{{this}}<a>
      {{/each}}
      </div>
    </div>
  </div>
  <div class="pull-right">
    <a href="#" class="numbee-icon"><i class="icon-heart"></i></a>
    <a href="#" class="numbee-icon"><i class="icon-star"></i></a>
    <a href="#" class="numbee-icon"><i class="icon-tag"></i></a>
  </div>
</div>
<hr>

